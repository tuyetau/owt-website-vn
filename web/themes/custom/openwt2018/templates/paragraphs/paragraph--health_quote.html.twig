{%
    set classes = [
    'paragraph section-health section--fluid-right',
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
]
%}
<section {{ attributes.addClass(classes) }}>
	  <div class="title-block">
			<h3 class="title-block__text">{{ content.field_title}}</h2>
			<div class="title-block__line title-block__line--red"></div>
		</div>
		<div class="quote-block quote-block--overlay-health quote-block--fluid">
      {{ content.field_health_quote }}
    </div>
</section>
